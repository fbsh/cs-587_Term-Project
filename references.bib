@article{he_receptor-binding_2004,
	title = {Receptor-binding domain of {SARS}-{CoV} spike protein induces highly potent neutralizing antibodies: implication for developing subunit vaccine},
	volume = {324},
	issn = {0006-291X},
	shorttitle = {Receptor-binding domain of {SARS}-{CoV} spike protein induces highly potent neutralizing antibodies},
