@inproceedings{das_fast_2019,
	address = {New York, NY, USA},
	series = {{SC} '19},
	title = {Fast, {Scalable} and {Accurate} {Finite}-{Element} {Based} {Ab} {Initio} {Calculations} {Using} {Mixed} {Precision} {Computing}: 46 {PFLOPS} {Simulation} of a {Metallic} {Dislocation} {System}},
	isbn = {978-1-4503-6229-0},
	shorttitle = {Fast, scalable and accurate finite-element based \textit{ab initio} calculations using mixed precision computing},
	url = {https://doi.org/10.1145/3295500.3357157},
	doi = {10.1145/3295500.3357157},
	abstract = {Accurate large-scale first principles calculations based on density functional theory (DFT) in metallic systems are prohibitively expensive due to the asymptotic cubic scaling computational complexity with number of electrons. Using algorithmic advances in employing finite-element discretization for DFT (DFT-FE) in conjunction with efficient computational methodologies and mixed precision strategies, we delay the onset of this cubic scaling by significantly reducing the computational prefactor while increasing the arithmetic intensity and lowering the data movement costs. This has enabled fast, accurate and massively parallel DFT calculations on large-scale metallic systems on both many-core and heterogeneous architectures, with time-to-solution being an order of magnitude faster than state-of-the-art plane-wave DFT codes. We demonstrate an unprecedented sustained performance of 46 PFLOPS (27.8\% peak FP64 performance) on a dislocation system in Magnesium containing 105,080 electrons using 3,800 GPU nodes of Summit supercomputer, which is the highest performance to-date among DFT codes.},
	urldate = {2021-05-31},
	booktitle = {Proceedings of the {International} {Conference} for {High} {Performance} {Computing}, {Networking}, {Storage} and {Analysis}},
	publisher = {Association for Computing Machinery},
	author = {Das, Sambit and Motamarri, Phani and Gavini, Vikram and Turcksin, Bruno and Li, Ying Wai and Leback, Brent},
	month = nov,
	year = {2019},
	keywords = {density functional theory, finite-elements, heterogeneous architectures, light-weight alloys, mixed precision, scalability},
	pages = {1--11},
}

@inproceedings{brunner_scalable_2000,
	address = {USA},
	series = {{SC} '00},
	title = {Scalable molecular dynamics for large biomolecular systems},
	isbn = {978-0-7803-9802-3},
	abstract = {We present an optimized parallelization scheme for molecular dynamics simulations of large biomolecular systems, implemented in the production-quality molecular dynamics program NAMD. With an object-based hybrid force and spatial decomposition scheme, and an aggressive measurement-based predictive load-balancing framework, we have attained speeds and speedups that are much higher than any reported in literature so far. The paper first summarizes the broad methodology we are pursuing, and the basic parallelization scheme we used. It then describes the optimizations that were instrumental in increasing performance, and presents performance results on benchmark simulations.},
