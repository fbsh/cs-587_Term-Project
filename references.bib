	journal = {Journal of Computational Chemistry},
	author = {Hughes, Zak E. and Ren, Emmanuel and Thacker, Joseph C. R. and Symons, Benjamin C. B. and Silva, Arnaldo F. and Popelier, Paul L. A.},
	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jcc.26111},
	keywords = {machine learning, molecular dynamics, multipole moments, quantum chemical topology, water simulation},
	pages = {619--628},
}

