@article{wang_allosteric_2014,
	title = {Allosteric {Response} and {Substrate} {Sensitivity} in {Peptide} {Binding} of the {Signal} {Recognition} {Particle}},
	volume = {289},
	issn = {0021-9258, 1083-351X},
	url = {http://www.jbc.org/lookup/doi/10.1074/jbc.M114.584912},
	doi = {10.1074/jbc.M114.584912},
	abstract = {We characterize the conformational dynamics and substrate selectivity of the signal recognition particle (SRP) using a thermodynamic free energy cycle approach and microsecond timescale molecular dynamics simulations. The SRP is a central component of the co-translational protein targeting machinery that binds to the N-terminal signal peptide (SP) of nascent proteins. We determined the shift in relative conformational stability of the SRP upon substrate binding to quantify allosteric coupling between SRP domains. In particular, for dipeptidyl aminopeptidase, an SP that is recognized by the SRP for co-translational targeting, it is found that substrate binding induces substantial changes in the SRP toward configurations associated with targeting of the nascent protein, and it is found that the changes are modestly enhanced by a mutation that increases the hydrophobicity of the SP. However, for alkaline phosphatase, an SP that is recognized for post-translational targeting, substrate binding induces the reverse change in the SRP conformational distribution away from targeting configurations. Microsecond timescale trajectories reveal the intrinsic flexibility of the SRP conformational landscape and provide insight into recent single molecule studies by illustrating that 10-nm lengthscale changes between FRET pairs occur via the rigid-body movement of SRP domains connected by the flexible linker region. In combination, these results provide direct evidence for the hypothesis that substrate-controlled conformational switching in the SRP provides a mechanism for discriminating between different SPs and for connecting substrate binding to downstream steps in the protein targeting pathway.},
	language = {en},
	number = {44},
	urldate = {2020-10-13},
	journal = {Journal of Biological Chemistry},
	author = {Wang, Connie Y. and Miller, Thomas F.},
	month = oct,
	year = {2014},
	pages = {30868--30879},
}

