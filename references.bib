@article{zhang_clinically_2020,
	title = {Clinically {Applicable} {AI} {System} for {Accurate} {Diagnosis}, {Quantitative} {Measurements}, and {Prognosis} of {COVID}-19 {Pneumonia} {Using} {Computed} {Tomography}},
	volume = {181},
